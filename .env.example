# Example environment variables for My Life As A Dev project
# Copy this file to .env and fill in your OpenAI API key

# Required for AI features
OPENAI_API_KEY=your_openai_api_key_here

# Optional settings with sensible defaults
# AI_MODEL controls which OpenAI model is used
AI_MODEL=gpt-4
# AI_TEMPERATURE sets response creativity (0.0 - 1.0)
AI_TEMPERATURE=0.7
